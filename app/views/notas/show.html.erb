         <td width =" 12%"><b>Disciplina:</b></td>
         <td> <%= session[:disciplina]%></td>
         <td width =" 12%"><b>Professor:</b></td>
         <td> <%= session[:professor]%></td>
      </tr>
       <tr bgcolor="#F5F5F5">
         
           <td width =" 12%"><b>Lançamentos:</b></td>
           <td colspan="3"><b>Nota:</b>&nbsp&nbsp<%= @nota.nota5%>
