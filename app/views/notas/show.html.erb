             <b>Faltas:</b>&nbsp&nbsp<%= @nota.faltas5 %>
         </td>
      </tr>
       <tr bgcolor="#F5F5F5">
         <td width =" 12%" valign="top"><b>Observação:</b></td>
         <td colspan="4"><%= @nota.obs1 %>
         </td>
      </tr>
    </table>
  </fieldset>
  <table width="100%" >
         <tr>
            <td width="6%"align="left" ><%= button_to 'Alterar' ,lancamentos_notas_notas_path, :title => "Alterar" %></td>
            <td><%= button_to 'Fechar', home_path, {:method => "get"}  %></td>

         </tr>
       </table>

</div>
