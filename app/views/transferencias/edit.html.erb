<h1>Editing transferencia</h1>

<% form_for(@transferencia) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :de %><br />
    <%= f.text_field :de %>
  </p>
  <p>
    <%= f.label :para %><br />
    <%= f.text_field :para %>
  </p>
  <p>
    <%= f.label :aluno_id %><br />
    <%= f.text_field :aluno_id %>
  </p>
  <p>
    <%= f.label :ano_letivo %><br />
    <%= f.text_field :ano_letivo %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @transferencia %> |
<%= link_to 'Back', transferencias_path %>