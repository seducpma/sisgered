<h1>Listing observacao_notas</h1>

<table>
  <tr>
    <th>Nota</th>
    <th>Observacao</th>
    <th>Data</th>
  </tr>

<% @observacao_notas.each do |observacao_nota| %>
  <tr>
    <td><%=h observacao_nota.nota %></td>
    <td><%=h observacao_nota.observacao %></td>
    <td><%=h observacao_nota.data %></td>
    <td><%= link_to 'Show', observacao_nota %></td>
    <td><%= link_to 'Edit', edit_observacao_nota_path(observacao_nota) %></td>
    <td><%= link_to 'Destroy', observacao_nota, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New observacao_nota', new_observacao_nota_path %>